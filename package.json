{
  "expo": {
    "name": "medsyncfrom",
    "slug": "medsyncfrom",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "permissions": [
      "LOCATION"
    ],
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicación para mostrar direcciones.",
        "NSLocationAlwaysUsageDescription": "Necesitamos tu ubicación para mostrar direcciones."
      }
    },
    "scripts": {
      "start": "expo start",
      "android": "expo start --android",
      "ios": "expo start --ios",
      "web": "expo start --web"
    }
  }
}
